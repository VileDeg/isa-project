{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/dns",
            //"args": ["-r", "-x", "-s", "dns.google", "2a00:1450:400d:806::2004"],
            //"args": ["-r", "-x", "-s", "dns.google", "15.197.204.56"],
            //"args": ["-r", "-x", "-s", "dns.google", "93.184.216.34"],
            //"args": ["-r", "-x", "-s", "dns.google", "www.example.com"],
            //"args": ["-r", "-x", "-s", "dns.google", "www.github.com"],
            "args": ["-r", "-s", "dns.google", "www.github.com"],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "build"
        }

    ]
}